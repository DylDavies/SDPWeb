{
  "name": "sdpweb",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:ssr": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "serve:ssr": "node dist/SDPWeb/server/server.mjs",
    "serve:ssr:SDPWeb": "node dist/SDPWeb/server/server.mjs",
    "lint": "ng lint",
    "docs": "typedoc",
    "start:static": "http-server -p 4200 ./dist/SDPWeb/",
    "analyze:size": "ng build --configuration=production && source-map-explorer dist/SDPWeb/browser/*.js"
  },
  "prettier": {
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^20.1.3",
    "@angular/cdk": "^20.1.3",
    "@angular/common": "^20.1.0",
    "@angular/compiler": "^20.1.0",
    "@angular/core": "^20.1.0",
    "@angular/forms": "^20.1.0",
    "@angular/material": "^20.1.3",
    "@angular/platform-browser": "^20.1.0",
    "@angular/platform-server": "^20.1.0",
    "@angular/router": "^20.1.0",
    "@angular/ssr": "^20.1.3",
    "compression": "^1.8.1",
    "express": "^5.1.0",
    "gridfs-stream": "^1.1.1",
    "jspdf": "^3.0.3",
    "jwt-decode": "^4.0.0",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "mongoose": "^8.18.1",
    "multer": "^2.0.2",
    "rxjs": "~7.8.0",
    "socket.io-client": "^4.8.1",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-eslint/builder": "^20.1.1",
    "@angular-eslint/schematics": "^20.1.1",
    "@angular/build": "^20.1.3",
    "@angular/cli": "^20.1.3",
    "@angular/compiler-cli": "^20.1.0",
    "@types/compression": "^1.8.1",
    "@types/express": "^5.0.1",
    "@types/jasmine": "~5.1.0",
    "@types/lodash": "^4.17.20",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.17.19",
    "angular-eslint": "20.1.1",
    "eslint": "^9.29.0",
    "jasmine-core": "~5.8.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "prettier": "^3.6.2",
    "source-map-explorer": "^2.5.3",
    "typedoc": "^0.28.10",
    "typescript": "~5.8.2",
    "typescript-eslint": "8.34.1"
  }
}
